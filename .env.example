# 高德地图API配置
AMAP_API_KEY=xxxxxxx
AMAP_API_BASE_URL=https://restapi.amap.com
AMAP_API_TIMEOUT=30s

# Spring配置
SPRING_PROFILES_ACTIVE=default

# 日志配置
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_AMAP_MCP=DEBUG

# 安全配置
APP_SECURITY_API_RATE_LIMIT=1000
APP_SECURITY_ALLOWED_ORIGINS=*

# JVM配置
JAVA_OPTS=-Xmx512m -Xms256m -XX:+UseG1GC